{
    "name": "opigno/opigno-composer",
    "description": "Opigno LMS",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "enyo/dropzone": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "v5.5.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dropzone/dropzone/archive/refs/tags/v5.5.0.zip",
                    "type": "zip"
                }
            }
        },
        "mozilla/pdf.js": {
            "type": "package",
            "package": {
                "name": "mozilla/pdf.js",
                "version": "v2.4.456",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mozilla/pdf.js/releases/download/v2.4.456/pdfjs-2.4.456-es5-dist.zip",
                    "type": "zip"
                }
            }
        },
        "kenwheeler/slick": {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/v1.8.1.zip",
                    "type": "zip"
                }
            }
        },
        "furf/jquery-ui-touch-punch": {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        "almende/vis": {
            "type": "package",
            "package": {
                "name": "almende/vis",
                "version": "v4.21.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/almende/vis/archive/refs/tags/v4.21.0.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "wikimedia/composer-merge-plugin": "2.0",
        "composer/installers": "*",
        "cweagans/composer-patches": "^1.7",
        "drupal/core-composer-scaffold": "~9.3.0",
        "opigno/opigno_lms": "~3.1.0",
        "drupal/admin_toolbar": "^3.2",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/colorbox": "^1.10",
        "drupal/colorbox_inline": "^1.2",
        "drupal/colorbox_load": "^1.3",
        "drupal/commerce_feeds": "^1.0@alpha",
        "drupal/commerce_paypal": "^1.0@RC",
        "drupal/commerce_stripe": "^1.0@RC",
        "drupal/csv_serialization": "^2.1",
        "drupal/custom_add_another": "^1.0@beta",
        "drupal/devel": "^4.2",
        "drupal/devel_php": "^1.3",
        "drupal/empty_fields": "^1.0@beta",
        "drupal/feeds_ex": "^1.0@beta",
        "drupal/form_options_attributes": "^1.4",
        "drupal/go_back_history": "^1.2",
        "drupal/modules_weight": "^1.9",
        "drupal/node_view_permissions": "^1.6",
        "drupal/pathauto": "^1.10",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/redirect_after_login": "^2.7",
        "drupal/redirect_after_logout": "^1.1",
        "drupal/serial": "^2.0@beta",
        "drupal/smtp": "^1.2",
        "drupal/stringoverrides": "^1.8",
        "drupal/stripe_api": "^1.0",
        "drupal/stripe_registration": "^3.0",
        "drupal/superfish": "^1.7",
        "drupal/twig_blocks": "^2.5",
        "drupal/twig_tweak": "^2.10",
        "drupal/upgrade_status": "^3.19",
        "drupal/user_csv_import": "^2.0",
        "drupal/views_conditional": "^1.4",
        "drupal/views_data_export": "^1.1",
        "drupal/views_daterange_filters": "^1.0@alpha",
        "drupal/webform": "^6.1.4",
        "drupal/xls_serialization": "^1.2",
        "drush/drush": "^11",
        "drupal/views_role_based_global_text": "^3.1",
        "drupal/multiselect": "^1.2",
        "drupal/ggroup": "^1.0@alpha",
        "drupal/ludwig": "^1.12"
    },
    "require-dev": {
        "drush/drush": "^10 || ^11",
        "drupal/console": "~1.0"
    },
    "replace": {
        "h5p/h5p-core": "*",
        "h5p/h5p-editor": "*"
    },
    "config": {
        "sort-packages": false,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/console-extend-plugin": true,
            "topfloor/composer-cleanup-vcs-dirs": true
        },
        "platform": {
            "php": "7.4.21"
        }

    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/opigno_statistics": {
                "#3265006": "https://www.drupal.org/files/issues/2023-08-30/drush-updb-failed-opigno_statistics_update_9001-3265006-4.patch"
            },
            "drupal/opigno_learning_path": {
                "#3366122": "https://www.drupal.org/files/issues/2023-08-30/Drush-update-failed-on-some-of-hook-updates-3366122-6.patch"
            },
            "drupal/opigno_messaging": {
                "#3326718": "https://www.drupal.org/files/issues/2023-08-30/Drush-updb-failed-on-hook-updates-9003-and-9004-3326718-6.patch"
            },
            "drupal/opigno_calendar": {
                "#3384258": "https://www.drupal.org/files/issues/2023-08-30/Drush-updb-failed-on-opigno_calendar_update_9001-3384258-2.patch"
            },
            "drupal/opigno_course": {
                "#3384256": "https://www.drupal.org/files/issues/2023-08-30/Drush-updb-failed-on-opigno_course_update_9001-3384256-2.patch"
            },
            "drupal/opigno_moxtra": {
                "#3384252": "https://www.drupal.org/files/issues/2023-08-30/Drush-updb-failed-on-opigno_moxtra_update_9004-3384252-2.patch"
            }
        },    
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/libraries/ckeditor": [
                "type:drupal-library-ckeditor"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": false,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
